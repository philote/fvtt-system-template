{
    "id": "fvtt-system-template",
    "title": "FVTT System Template",
    "description": "A <system name> system for Foundry VTT.",
    "version": "#{VERSION}#",
    "compatibility": {
      "minimum": 11,
      "verified": "11.315",
      "maximum": 12
    },
    "authors": [
      {
        "name": "ephson",
        "url": "https://github.com/philote",
        "discord": "ephson",
        "reddit": "u/ephson"
      }
    ],
    "esmodules": ["module/fvtt-system-template.mjs"],
    "styles": ["css/fvtt-system-template.css"],
    "languages": [
      {
        "lang": "en",
        "name": "English",
        "path": "languages/en.json"
      }
    ],
    "url": "#{URL}#",
    "manifest": "#{MANIFEST}#",
    "download": "#{DOWNLOAD}#",
    "license": "LICENSE",
    "readme": "README.md",
    "packs": [],
    "packFolders": [],
    "socket": false,
    "gridDistance": 5,
    "gridUnits": "ft",
    "primaryTokenAttribute": "health",
    "secondaryTokenAttribute": "power",
    "background": "systems/fvtt-system-template/assets/package_image.webp",
    "media": [
      {
        "type": "screenshot",
        "url": "https://raw.githubusercontent.com/philote/fvtt-system-template/main/assets/CD_screenshot.webp",
        "loop": false,
        "caption": "<system name> character sheet screenshot from Foundry VTT."
      },
      {
        "type": "cover",
        "url": "https://raw.githubusercontent.com/philote/fvtt-system-template/main/assets/cover.webp",
        "loop": false,
        "caption": "Cover Images for the <system name> package."
      },
      {
        "type": "setup",
        "url": "https://raw.githubusercontent.com/philote/fvtt-system-template/main/assets/package_image.webp",
        "thumbnail": "https://raw.githubusercontent.com/philote/fvtt-system-template/main/assets/package_image_thumb.webp"
      }
    ]
  }
  